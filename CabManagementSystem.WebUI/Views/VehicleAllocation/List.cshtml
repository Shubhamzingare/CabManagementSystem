@model CabManagementSystem.WebUI.Models.VehicalAllocationListViewModel

@{
    ViewBag.Title = "List Of Vehicle Allocation";
}
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</head>
<body>
    <div>
        <h3>Employee Detail</h3>
        <table class="table table-bordered table-responsive table-condensed">
            <thead>
                <tr class="btn-primary">
                    <th>vehicleAlloId</th>
                    <th>vehicleId</th>
                    <th>empId</th>
                    <th>driverId</th>
                    <th>PickOrDrop</th>
                    <th>routeId</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var p in Model.VehicleAllocationDetails)

                {
                    <tr>
                        <th>@p.vehicleAlloId</th>
                        <th>@p.vehicleId</th>
                        <th>@p.empId</th>
                        <th>@p.driverId</th>
                        <th>@p.PickOrDrop</th>
                        <th>@p.routeId</th>

                    </tr>
                }
            </tbody>

        </table>
    </div>
    <div class="btn-group pull-right" align="right">
        @Html.PageLinks(Model.PagingInfo, x => Url.Action("VehicleDetailList", new { page = x }))

    </div>

</body>

